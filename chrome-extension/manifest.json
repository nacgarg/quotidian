{
    "manifest_version": 2,
    "name": "Quotidian",
    "version": "1.2.0",
    "description": "Quotes that are a click away.",
    "browser_action": {
        "default_icon": {
            "19": "quotidian.png",
            "38": "quotidian.png"
        },
        "default_title": "Quotidian",
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://api.forismatic.com/",
        "tts",
        "clipboardWrite"
    ]
}
